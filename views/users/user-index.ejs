<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- Bootstrap -->
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
        <!-- Font Awesome -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.0-2/css/all.min.css" integrity="sha256-46r060N2LrChLLb5zowXQ72/iKKNiw/lAmygmHExk/o=" crossorigin="anonymous" />
        <!-- Main CSS -->
        <link rel="stylesheet" href="/styles/main.css">
        <!-- App JS -->
        <script defer src="/js/app.js"></script>
    <title>Dashboard</title>
</head>
<body>

    <header>
       
    </header>

    <main>
        <div class="container">
            <h1>My Dashboard</h1>
        </div>

        <div class="container">
            <table class="table table-striped">
                <thead>
                    
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">First</th>
                    <th scope="col">Last</th>
                    <th scope="col">Email</th>
                    <th scope="col"> Added</th>
                </tr>
                </thead>
                <tbody>
                    <% users.forEach((user, index) => { %>
                <tr>
                    <th scope="row"><%= index+1 %></th>
                    <td><%= user.firstName %></td>
                    <td><%= user.lastName %></td>
                    <td><a class= "email-links" href="mailto:<%=user.email%>"><%=user.email%> </a></td>
                    <td><%= user.createdAt.toDateString() %> 
                    <%= user.createdAt.toLocaleTimeString() %></td>
                </tr>
                <% }) %>
            
                </tbody>
            </table>
        </div>
        
    </main>



    <footer class="footertwo">
        <a href="#"><i class="fab fa-facebook m-1"></i></a>
        <a href="#"><i class="fab fa-instagram m-1"></i></a>
        <a href="#"><i class="fab fa-twitter-square m-1"></i></a>
        <a href=""><i class="fab fa-audible m-1"></i></a>
        <a href=""><i class="fas fa-podcast m-1"></i></a>
        <p>&copy; 2020 CAMP HUMOR PRODUCTIONS | | Truth is Funnier Than Fiction</p>
        <p>Privacy | Terms | Permission | Credit</p>
    </footer>

</body>
</html>